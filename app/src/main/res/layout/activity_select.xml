<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/_0017_background"
    tools:context=".SelectActivity">

    <ImageView
        android:id="@+id/imageView42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_0016_topline" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:fontFamily="@font/mapopeacefull"
        android:text="산책 옵션 설정"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView42"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="180dp"
        android:layout_height="150dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView43"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/startwalk_0006_cat" />

    <TextView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="140dp"
        android:drawableLeft="@drawable/_0010_dot"
        android:fontFamily="@font/mapopeacefull"
        android:text="  경로 추천 방식"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView16" />

    <TextView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:drawableLeft="@drawable/_0010_dot"
        android:fontFamily="@font/mapopeacefull"
        android:text="  산책 예상 시간"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView42" />

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/_0011_bottomline" />

    <ImageButton
        android:id="@+id/sub_btn"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="100dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/time_view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/time_view"
        app:srcCompat="@drawable/_0006_minus" />

    <ImageButton
        android:id="@+id/add_btn"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="100dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sub_btn"
        app:srcCompat="@drawable/_0003_plus" />

    <EditText
        android:id="@+id/time_view"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="40dp"
        android:background="#00FFFFFF"
        android:fontFamily="@font/mapo"
        android:gravity="right"
        android:hint="0"
        android:inputType="number"
        android:maxLength="3"
        android:selectAllOnFocus="true"
        android:text="30"
        android:textColor="#548EF8"
        android:textSize="26sp"
        app:layout_constraintStart_toEndOf="@+id/sub_btn"
        app:layout_constraintTop_toBottomOf="@+id/imageView16" />

    <TextView
        android:id="@+id/t"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="40dp"
        android:background="#00FFFFFF"
        android:fontFamily="@font/mapo"
        android:text="분"
        android:textColor="#548EF8"
        android:textSize="26sp"
        app:layout_constraintStart_toEndOf="@+id/time_view"
        app:layout_constraintTop_toBottomOf="@+id/imageView16" />

    <Button
        android:id="@+id/back_btn"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:background="#00FFFFFF"
        android:text="취소"
        android:fontFamily="@font/mapopeacefull"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/startwalk_0003_cancel" />

    <Button
        android:id="@+id/start_btn"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:background="#00FFFFFF"
        android:fontFamily="@font/mapopeacefull"
        android:text="경로 확인"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <CheckBox
        android:id="@+id/record_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:button="@drawable/mycheck"
        android:checked="true"
        android:fontFamily="@font/mapopeacefull"
        android:text="  이전 경로 1"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView15" />

    <CheckBox
        android:id="@+id/record_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:button="@drawable/mycheck"
        android:checked="false"
        android:fontFamily="@font/mapopeacefull"
        android:text="  이전 경로 2"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/record_1"
        app:layout_constraintTop_toBottomOf="@+id/record_1" />

    <CheckBox
        android:id="@+id/record_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:button="@drawable/mycheck"
        android:checked="false"
        android:fontFamily="@font/mapopeacefull"
        android:text="  이전 경로 3"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/record_2"
        app:layout_constraintTop_toBottomOf="@+id/record_2" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:button="@drawable/mycheck"
        android:checked="false"
        android:fontFamily="@font/mapopeacefull"
        android:text="  추천 경로"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/record_3"
        app:layout_constraintTop_toBottomOf="@+id/record_3" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:button="@drawable/mycheck"
        android:checked="false"
        android:fontFamily="@font/mapopeacefull"
        android:text="  직접 선택"
        android:textColor="#0072FF"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/checkBox2"
        app:layout_constraintTop_toBottomOf="@+id/checkBox2" />

</androidx.constraintlayout.widget.ConstraintLayout>