<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainmenu_background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenu">

    <ImageView
        android:id="@+id/iv_window"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/dayclear"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/main_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/iv_window"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="@+id/iv_window"
        app:srcCompat="@drawable/mainday" />

    <ImageButton
        android:id="@+id/notice_btn"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/articlec"
        app:layout_constraintEnd_toEndOf="@+id/btn_config"
        app:layout_constraintStart_toStartOf="@+id/btn_config"
        app:layout_constraintTop_toBottomOf="@+id/btn_config" />

    <Button
        android:id="@+id/btn_community"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="4dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.714" />

    <Button
        android:id="@+id/btn_to_select"
        android:layout_width="200dp"
        android:layout_height="300dp"
        android:layout_marginEnd="4dp"
        android:background="#00ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.614" />

    <ImageButton
        android:id="@+id/btn_rank"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/rankc"
        app:layout_constraintEnd_toStartOf="@+id/btn_pay"
        app:layout_constraintTop_toTopOf="@+id/btn_pay" />

    <ImageButton
        android:id="@+id/btn_pay"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/npayc"
        app:layout_constraintEnd_toStartOf="@+id/btn_config"
        app:layout_constraintTop_toTopOf="@+id/btn_config" />

    <ImageButton
        android:id="@+id/btn_config"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/configc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_info"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/main__0015_info"
        app:layout_constraintEnd_toStartOf="@+id/btn_rank"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_onoff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/infoon"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_laptop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/iv_window"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="@+id/iv_window"
        app:srcCompat="@drawable/laptop2" />

    <ImageView
        android:id="@+id/iv_cat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/taetaebig"
        app:layout_constraintBottom_toBottomOf="@+id/iv_window"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_figure1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_1" />

    <ImageView
        android:id="@+id/iv_figure2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_2" />

    <ImageView
        android:id="@+id/iv_figure3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_3" />

    <ImageView
        android:id="@+id/iv_figure4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_4" />

    <ImageView
        android:id="@+id/iv_figure5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_5" />

    <ImageView
        android:id="@+id/iv_texton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/main_texton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>