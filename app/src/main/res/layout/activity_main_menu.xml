<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainmenu_background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main__0017_grey"
    tools:context=".MainMenu">

    <ImageView
        android:id="@+id/imageView54"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/btn_to_select"
        app:layout_constraintStart_toStartOf="@+id/btn_to_select"
        app:layout_constraintTop_toTopOf="@+id/btn_to_select"
        app:srcCompat="@drawable/main__0016_doortag" />

    <Button
        android:id="@+id/btn_community"
        android:layout_width="95dp"
        android:layout_height="115dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/_0003_community"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_window" />

    <Button
        android:id="@+id/btn_to_select"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="65dp"
        android:layout_marginBottom="85dp"
        android:background="#00ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_itemboard"
        android:layout_width="165dp"
        android:layout_height="380dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/_0000_popo_lay"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/iv_window"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toTopOf="@+id/btn_itemboard"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main__0001_dayclear" />

    <Button
        android:id="@+id/btn_rank"
        android:layout_width="95dp"
        android:layout_height="115dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/_0002_ranking"
        app:layout_constraintStart_toStartOf="@+id/btn_community"
        app:layout_constraintTop_toBottomOf="@+id/iv_window" />

    <Button
        android:id="@+id/btn_pay"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/pay"
        app:layout_constraintEnd_toStartOf="@+id/btn_config"
        app:layout_constraintTop_toTopOf="@+id/btn_config" />

    <ImageView
        android:id="@+id/imageView56"
        android:layout_width="200dp"
        android:layout_height="150dp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_window"
        app:layout_constraintEnd_toEndOf="@+id/iv_window"
        app:layout_constraintStart_toStartOf="@+id/iv_window"
        app:layout_constraintTop_toTopOf="@+id/iv_window"
        app:srcCompat="@drawable/main__0000_xwindo" />

    <Button
        android:id="@+id/btn_config"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/main__0014_setting"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_info"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/main__0015_info"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_pay" />

</androidx.constraintlayout.widget.ConstraintLayout>