<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="300dp"
    android:layout_height="145dp"
    android:background="@drawable/delete_0003_box">


    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:fontFamily="@font/mapopeacefull"
        android:gravity="center"
        android:text="게시글을 삭제하시겠습니까?"
        android:textColor="#1A62BB"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/can_btn"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="25dp"
        android:layout_weight="1"
        android:background="@drawable/delete_0000_cancel"
        android:foreground="@drawable/delete_0000_cancel"
        android:gravity="center"
        android:onClick="mOnClose"
        android:textColor="#ff7a00"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/del_btn"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="25dp"
        android:layout_weight="1"
        android:background="@drawable/delete_0002_delete"
        android:gravity="center"
        android:onClick="mOnClose"
        android:textColor="#ff7a00"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>